{
  "version": 2,
  "builds": [
    {
      "src": "cmd/server/main.go",
      "use": "vercel-go@1.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/cmd/server/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/cmd/server/main.go"
    }
  ],
  "env": {
    "PORT": "8080"
  }
}
